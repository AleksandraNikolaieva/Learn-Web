.backdrop('*ngIf'='isNavMenuOpen' '(click)'='closeNavMenu()')
nav('[ngClass]'='{"slideIn": isNavMenuOpen}')
    ul
        li('*ngFor'='let item of menuItems')
            a(routerLink='{{item.link}}' routerLinkActive="active")
                i(class='{{item.icon}}')
                span {{item.title}}
