app-viewport('link'='constructor')
    div(main)
        app-quizz-card('*ngFor'='let quizz of quizzes$ | async; let i = index'
        '[quizz]'='quizz'
        '[id]'='i'
        '[loggedUser]'='loggedUser$ | async'
        '(deleted)'='deleteQuizz($event)')
    router-outlet(name='aside' auxiliary)